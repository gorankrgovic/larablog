{
    "name": "gorankrgovic/larablog",
    "type": "library",
    "description": "Larablog is simple but yet powerful Blog package for the Laravel 5.6 and up.",
    "keywords": [
        "Laravel",
        "Blog",
        "Larablog"
    ],
    "homepage": "https://github.com/gorankrgovic/larablog",
    "license": "MIT",
    "authors": [
        {
            "name": "Goran Krgovic",
            "email": "gorankrgovic1@gmail.com",
            "homepage": "http://digitalinfinity.rs",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1.3",
        "kkszymanowski/traitor": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Gorankrgovic\\Larablog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gorankrgovic\\Larablog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gorankrgovic\\Larablog\\LarablogServiceProvider"
            ],
            "aliases": {
                "Gorankrgovic\\Larablog": "Gorankrgovic\\Larablog\\Facades\\Larablog"
            }
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    }
}
